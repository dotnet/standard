        "branchFilters": "[\"+refs/heads/*\"]",
        "additionalFields": "{}"
      }
    }
  ],
  "variables": {
    "system.debug": {
      "value": "false",
      "allowOverride": true
    },
    "TeamName": {
      "value": "DotNetCore"
    },
    "CloudDropAccountName": {
      "value": "dotnetbuildoutput"
    },
    "OfficialBuildId": {
      "value": "$(Build.BuildNumber)"
    },
    "Label": {
      "value": "$(Build.BuildNumber)"
    },
    "VsoAccountName": {
      "value": "dn-bot"
    },
    "VsoPassword": {
      "value": null,
