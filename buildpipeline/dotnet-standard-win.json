    },
    "SourceBranch": {
      "value": "master"
    },
    "PB_GitHubAuthToken": {
      "value": null,
      "isSecret": true
    },
    "PB_AzureFeedUrl": {
      "value": "",
      "isSecret": true
    },
    "PB_AzureSymbolFeedUrl": {
      "value": "",
